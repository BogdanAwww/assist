	"scripts": {
		"dev-static": "webpack serve",
		"dev-server": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node-dev -r tsconfig-paths/register --no-notify --respawn --watch src/server src/server/app.ts",
		"dev-admin": "cross-env ENTRY=admin npm run dev-static",
		"dev-web": "cross-env ENTRY=web npm run dev-static",
		"build-static": "webpack",
		"build-admin": "rm -rf build/admin && cross-env ENTRY=admin npm run build-static",
		"build-web": "rm -rf build/web && cross-env ENTRY=web npm run build-static",
		"build-server": "rm -rf build/server && tsc -p ./tsconfig.server.json && npm run copy-server",
		"scheduler": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node -r tsconfig-paths/register tools/scheduler/index.ts",
		"migrate-countries": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node -r tsconfig-paths/register tools/migration/countries/index.ts",
		"migrate-cities": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node -r tsconfig-paths/register tools/migration/cities/index.ts",
		"migrate-specialties": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node -r tsconfig-paths/register tools/migration/specialties/index.ts",
		"migrate-specialties-freq": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node -r tsconfig-paths/register tools/migration/specialties/frequently.ts",
		"migrate-assist-helper": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node -r tsconfig-paths/register tools/migration/assist-helper/index.ts",
		"migrate-database": "cross-env TS_NODE_PROJECT=src/server/tsconfig.json ts-node -r tsconfig-paths/register tools/migration/index.ts",
		"copy-server": "copyfiles -e '**/*.ts' -e '**/.gitkeep' -e '**/tsconfig.json' -u 2 'src/server/**/*.*' build/server",
		"check": "npm run check-ts && npm run check-lint",
		"check-ts": "tsc --noEmit --skipLibCheck -p ./tsconfig.json",
		"check-lint": "eslint ./src --ext .ts,.tsx,.js",
		"watch": "DEBUG=axios:* jest --env node --watch",
		"test": "jest --env node",
		"build": "tsc -p ./tsconfig.build.json",
		"gen-schema": "ts-node ./stuff/bonus/generateSchema",
		"gen-sdk": "yarn gen-schema && yarn graphql-codegen -c ./stuff/bonus/codegen.yml",
		"start": "ts-node -T src/server.ts"
	},
